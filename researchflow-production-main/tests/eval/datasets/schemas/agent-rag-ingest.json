{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "agent-rag-ingest-response",
  "title": "agent-rag-ingest response",
  "description": "Document ingestion result with chunk/document counts.",
  "allOf": [{ "$ref": "_base_envelope.json" }],
  "properties": {
    "outputs": {
      "type": "object",
      "properties": {
        "ingestedCount": { "type": "integer", "minimum": 0 },
        "chunkCount": { "type": "integer", "minimum": 0 },
        "collection": { "type": "string" },
        "docIds": { "type": "array", "items": { "type": "string" } },
        "chunkIds": { "type": "array", "items": { "type": "string" } },
        "errors": { "type": "array", "items": { "type": "string" } }
      }
    }
  }
}
