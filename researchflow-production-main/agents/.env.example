# ResearchFlow Agent Environment Variables
# Copy to .env and fill in your values

# ========== REQUIRED ==========

# Composio API Key (get from https://app.composio.dev/settings/api-keys)
COMPOSIO_API_KEY=

# OpenAI API Key (for GPT-4o models)
OPENAI_API_KEY=

# ========== OPTIONAL (for specific agents) ==========

# Anthropic API Key (if using Claude models)
ANTHROPIC_API_KEY=

# ========== FIGMA (for DesignOps Agent) ==========

# Figma Personal Access Token
FIGMA_ACCESS_TOKEN=

# Figma file key (from URL: figma.com/file/{FILE_KEY}/...)
FIGMA_FILE_KEY=

# ========== GITHUB ==========

# GitHub Personal Access Token
GITHUB_TOKEN=

# Repository in format owner/repo
GITHUB_REPO=ry86pkqf74-rgb/researchflow-production

# ========== NOTION (for SpecOps, Compliance agents) ==========

# Notion Integration Token
NOTION_API_KEY=

# Notion Database IDs (get from database URL)
NOTION_PROBLEM_REGISTRY_DB=59c645dc21d44ca0baa0dd8a40f8338f
NOTION_DATASET_REGISTRY_DB=97cac1c2a79f495d9872d9b948eb8854
NOTION_MODEL_REGISTRY_DB=5f3917dded164e10b1cfa2c97fb965b2
NOTION_RISK_REGISTER_DB=609acd3a992b46e4b612366c1f586dd1
NOTION_DEPLOYMENTS_DB=187da44af9f94f35aeaa9c99727ff733
NOTION_INCIDENTS_DB=13f3cd27551f4672a5c2eaef91cfa5b6

# ========== LANGSMITH (for tracing) ==========

LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=
LANGCHAIN_PROJECT=researchflow-agents

# ========== SLACK (for notifications) ==========

SLACK_WEBHOOK_URL=
SLACK_CHANNEL_COMPLIANCE=compliance-updates

# ========== CUSTOM AGENT MODELS ==========

WIRING_AUDIT_MODEL=gpt-4o-mini
WIRING_AUDIT_TEMPERATURE=0
ORCHESTRATION_FIX_MODEL=gpt-4o
ORCHESTRATION_FIX_TEMPERATURE=0
DOCKER_STACK_MODEL=gpt-4o
DOCKER_STACK_TEMPERATURE=0
