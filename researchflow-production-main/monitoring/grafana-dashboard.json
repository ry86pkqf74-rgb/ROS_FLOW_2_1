{
  "id": null,
  "uid": "researchflow-monitoring",
  "title": "ResearchFlow - Monitoring",
  "tags": ["researchflow", "observability", "prometheus"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": { "from": "now-6h", "to": "now" },
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_PROMETHEUS",
        "query": "prometheus",
        "current": { "selected": true, "text": "Prometheus", "value": "Prometheus" }
      },
      {
        "type": "query",
        "name": "job",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": { "query": "label_values(up, job)", "refId": "PromJob" },
        "refresh": 2,
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "current": { "selected": true, "text": "All", "value": "$__all" }
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "Workflow Overview",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "stat",
      "title": "Active Workflows",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 1 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "auto", "textMode": "auto", "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "none", "min": 0 }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(ros_active_workflows)",
          "legendFormat": "active"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Stage Executions (completed, 30m)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 5, "w": 6, "x": 6, "y": 1 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "auto", "textMode": "auto", "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "none", "min": 0 }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(increase(ros_stage_executions_total{status=\"completed\"}[30m]))",
          "legendFormat": "completed"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Stage Executions (failed, 30m)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 5, "w": 6, "x": 12, "y": 1 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "auto", "textMode": "auto", "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "none", "min": 0, "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null }, { "color": "red", "value": 1 } ] } }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(increase(ros_stage_executions_total{status=\"failed\"}[30m]))",
          "legendFormat": "failed"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Total Stage Executions (24h)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 5, "w": 6, "x": 18, "y": 1 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "auto", "textMode": "auto", "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "none", "min": 0 }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(increase(ros_stage_executions_total[24h]))",
          "legendFormat": "total"
        }
      ]
    },
    {
      "type": "row",
      "title": "Workflow & Stage Performance",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 6 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "heatmap",
      "title": "Stage Execution Heatmap (p95 bucketed, last 6h)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 7 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (le, stage_id) (rate(ros_stage_duration_seconds_bucket[5m]))",
          "legendFormat": "{{stage_id}}"
        }
      ],
      "options": {
        "color": { "mode": "scheme", "scheme": "Spectral" },
        "heatmap": { "xBucketNumber": 30, "yBucketNumber": 10, "yBucketSize": 1 },
        "legend": { "show": true },
        "tooltip": { "show": true }
      }
    },
    {
      "type": "timeseries",
      "title": "Error Rate by Stage (5m)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 10, "w": 12, "x": 12, "y": 7 },
      "fieldConfig": { "defaults": { "unit": "percentunit" }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (stage_id) (rate(ros_stage_executions_total{status=\"failed\"}[5m])) / clamp_min(sum by (stage_id) (rate(ros_stage_executions_total[5m])), 1e-9)",
          "legendFormat": "{{stage_id}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Average Stage Duration (s) (5m)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 17 },
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (stage_id) (rate(ros_stage_duration_seconds_sum[5m])) / clamp_min(sum by (stage_id) (rate(ros_stage_duration_seconds_count[5m])), 1e-9)",
          "legendFormat": "{{stage_id}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Stage Duration p95 (s)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 17 },
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le, stage_id) (rate(ros_stage_duration_seconds_bucket[5m])))",
          "legendFormat": "{{stage_id}}"
        }
      ]
    },
    {
      "type": "row",
      "title": "Queues & Resources",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 26 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Queue Depth Over Time",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 27 },
      "fieldConfig": { "defaults": { "unit": "none", "min": 0 }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (queue) (bullmq_queue_waiting) + sum by (queue) (bullmq_queue_active) + sum by (queue) (bullmq_queue_delayed)",
          "legendFormat": "{{queue}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Resource Utilization (CPU / Memory)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 27 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (job) (rate(process_cpu_seconds_total{job=~\"$job\"}[5m]))",
          "legendFormat": "cpu {{job}}"
        },
        {
          "refId": "B",
          "expr": "sum by (job) (process_resident_memory_bytes{job=~\"$job\"})",
          "legendFormat": "mem {{job}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "none" },
        "overrides": [
          { "matcher": { "id": "byRegexp", "options": ".*mem.*" }, "properties": [ { "id": "unit", "value": "bytes" } ] },
          { "matcher": { "id": "byRegexp", "options": ".*cpu.*" }, "properties": [ { "id": "unit", "value": "cores" } ] }
        ]
      }
    },
    {
      "type": "row",
      "title": "HTTP Request Rate & Latency",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 35 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Request Rate (req/s)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 36 },
      "fieldConfig": { "defaults": { "unit": "reqps", "min": 0 }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (job) (rate(researchflow_orchestrator_http_requests_total{job=~\"$job\"}[1m]))",
          "legendFormat": "{{job}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Request Latency (p50/p95/p99) (s)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 36 },
      "fieldConfig": { "defaults": { "unit": "s", "min": 0 }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.50, sum by (le, job) (rate(researchflow_orchestrator_http_request_duration_seconds_bucket{job=~\"$job\"}[5m])))",
          "legendFormat": "p50 {{job}}"
        },
        {
          "refId": "B",
          "expr": "histogram_quantile(0.95, sum by (le, job) (rate(researchflow_orchestrator_http_request_duration_seconds_bucket{job=~\"$job\"}[5m])))",
          "legendFormat": "p95 {{job}}"
        },
        {
          "refId": "C",
          "expr": "histogram_quantile(0.99, sum by (le, job) (rate(researchflow_orchestrator_http_request_duration_seconds_bucket{job=~\"$job\"}[5m])))",
          "legendFormat": "p99 {{job}}"
        }
      ]
    }
  ]
}
